  function gen_stat(ri,ci,type,mod,level) {
    //init
    var out = [{"type":"total","value":0,"note":0},{"type":"race","value":0,"note":0},{"type":"class","value":0,"note":0}];
    var typelist = ["feature","speed"];
    //race
    for(var i=0;i<obj_race[ri].race.mechanics.length;i++) {
      var valid = 0;
      for(var j=0;j<typelist.length;j++) {
        if(obj_race[ri].race.mechanics[i].type == typelist[j]) { valid = 1; }
      }
      if(valid == 1 && obj_race[ri].race.mechanics[i].result) {
        for(var j=0;j<obj_race[ri].race.mechanics[i].result.length;j++) {
          var rvar = obj_race[ri].race.mechanics[i].result[j];
          if(rvar.type=='stat_inc' && rvar.stat_type==type) {
            if(rvar.value) { out[1].value += parseFloat(rvar.value); }
            else if(rvar.stat) { out[1].value += parseFloat(mod[rvar.stat]); }
            if(rvar.note) {
              out[1].note = 1;
              out[1].text = rvar.text;
            }
          }
        }
      }
    }
    //class
    for(var i=0;i<obj_class[ci].class.mechanics.length;i++) {
      var valid = 0;
      for(var j=0;j<typelist.length;j++) {
        if(obj_class[ci].class.mechanics[i].type == typelist[j]) { valid = 1; }
      }
      if(valid == 1 && obj_class[ci].class.mechanics[i].result) {
        for(var j=0;j<obj_class[ci].class.mechanics[i].result.length;j++) {
          var rvar = obj_class[ci].class.mechanics[i].result[j];
          if(rvar.type=='stat_inc' && rvar.stat_type==type) {
            if(rvar.value||rvar.value===0) {
              //bard/ranger +1/2 class min 1 on knowledge
              if(rvar.value==0.5) {
                var add = Math.floor(0.5*(level));
                if(add<1) { add = 1; }
                out[2].value += add;
              } 
              
              else if(type=='fort'||type=='ref'||type=='will') {
                if(rvar.value==0) { out[2].value += Math.floor(level*(1/3)); }
                else if(rvar.value==1) {out[2].value += 2 + Math.floor(level*(1/2)); }
              }
              
              //else if(rvar.value.length>1&&rvar.value.constructor===Array) {
              //  out[2].value += parseFloat(rvar.value[level-1]);
              //}                
                else {
                out[2].value += parseFloat(rvar.value);
              }
            }
            else if(rvar.stat) { out[2].value += parseFloat(mod[rvar.stat]); }
            if(rvar.note) {
                out[2].note = 1;
                out[2].text = rvar.text;
            }
          }
        }
      }
    }
    //total
    var total = 0;
    for(var i=0;i<out.length;i++) {
      if(out[i].note == 1) {}
      else { total += parseFloat(out[i].value); }
    }
    out[0].value = total;    
    return out;
  }

