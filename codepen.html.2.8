      <a href="#defense_wrap" class="button secondary columns small-5">Defense</a>
    </div>
    <div class="row" id="quality_wrap">
    </div>
    <div class="row" id="storage_wrap">
      <div class="save"><img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/333520/save2.png"/></div>
    </div>
    <div class="row" id="modal_wrap">
      <div class="columns small-11 small-centered">
        <div class="modal_control"><a class="close_modal">X</a></div>
        <div class="modal"></div>
      </div>
    </div>
    <div class="row" id="general_wrap">
      <div class="general columns small-11 small-centered"></div>
    </div>
    <div class="row" id="defense_wrap">
      <div class="defense columns small-11 small-centered">
        <div class="row">
          <div class="columns small-12 medium-4 ch_obj" id="fort"></div>
          <div class="columns small-12 medium-4 ch_obj" id="ref"></div>
          <div class="columns small-12 medium-4 ch_obj" id="will"></div>    
        </div>
        <div class="row">
          <div class="columns small-12 medium-4 ch_obj" id="ac"></div>
          <div class="columns small-12 medium-4 ch_obj" id="ac_flat"></div>
          <div class="columns small-12 medium-4 ch_obj" id="ac_touch"></div>              
        </div>
        <div class="row">
          <div class="columns small-4 small-centered ch_obj" id="cmd"></div>
        </div>
      </div>
    </div>
    <div class="row" id="offense_wrap">
      <div class="offense columns small-11 small-centered">
        <div class="row ch_obj" id="cmb"></div>
        <div class="row ch_obj" id="bab"></div>
      </div>
    </div>    
    <div class="row" id="powers_wrap">
      <div class="columns small-11 small-centered">
        <div class="row ch_obj" id="proficiency-a"></div>
        <div class="row ch_obj" id="proficiency-w"></div>
        <div class="row ch_obj" id="speed"></div>
        <div class="row ch_obj" id="size"></div>
        <div class="row ch_obj" id="vision"></div>
        <div class="powers-actual"></div>
      </div>
    </div>
    <div class="row" id="stats_wrap">
      <div class="columns small-11 small-centered">
        <table></table>
      </div>
    </div>
    <div class="row" id="skills_wrap">
      <div class="columns small-11 small-centered">
        <table></table>
      </div>
    </div>    
    <div class="row" id="feats_wrap">

    </div>
    <div class="row" id="spells_wrap">

    </div>
</div>
  <div class="row" id="list"><h3>Saved Characters <a class="clear">DELETE ALL</a></h3><ul></ul></div>
  <div class="row" id="gen">
    <div class="gen small-12 columns">
      <ul class="control">
        <li><a class="button go">Click</a></li>
        <li>
          <select class="pt_val">
            <option>15</option>
            <option>20</option>
            <option>25</option>
          </select>
        </li>
        <li>
          <select class="trait_num">
            <option>3</option>
            <option>4</option>
            <option>5</option>
          </select>
        </li>
        <li>
          <select class="level_num">
            <option>1</option>
            <option>2</option>
            <option>3</option>
          </select>
        </li>
        <li><select class="c_name"><option>any class</option></select></li>
        <li><select class="r_name"><option>any race</option></select></li>
      </ul>
    </div>
    
